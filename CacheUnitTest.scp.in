;initialization -- turn match off to make comparisons more lenient
case match: off

;set the title
title: ${package_name}
display:Log file name is <P1><CR><LF>
;log everthing in a log, log file name will be passed in as parameter 1
logfile:<P1>
on error: $Exit
; set up a timer for 30 minutes (30*60*10), should be more than enough to finish
timer 18000
wait for:>
; switch to the right namespace
send: znspace "${NAMESPACE}"<CR>
wait for:${NAMESPACE}>
;;; set up the IO that is need by unit test
send:S DUZ=1 D Q^DI<CR>
wait for:Select OPTION:
send:<CR>
wait for:${NAMESPACE}>
send: D ^%ZIS<CR>
wait for: DEVICE:
send:TELNET;300;999<CR>
wait for:${NAMESPACE}>
${send_commands}
$Exit:
    timer 0
    ; close the log file
    closelog
    ; exit the terminal
    terminate
